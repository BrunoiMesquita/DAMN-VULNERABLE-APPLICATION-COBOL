//COPY.SYSUT1 DD DATA,DLM=@@
      *///////////////////////////////////////////////////////////////
      * Mel's Cargo
      * MCORDERS:
      *   Display and order supplies
      *
      * Vulnerabilities:
      *   Though the display field is locked you can change the price
      *   of any item by unlocking the screen.
      *
      *
      * AUTHOR:
      *   Philip Young aka Soldier of FORTRAN
      *
      * 09/17/2022
      * License GPL v3
      *///////////////////////////////////////////////////////////////
       IDENTIFICATION DIVISION.
       PROGRAM-ID.   MCMMENU.
       AUTHOR. SOLDIER OF FORTRAN.
       INSTALLATION. DAMN VULNERABLE.
       DATE-WRITTEN. 09/17/2022.
       SECURITY. CONFIDENTIAL.
       ENVIRONMENT DIVISION.
       DATA DIVISION.
       WORKING-STORAGE SECTION.
       01  MC-COMMS.
           05  PURCHASE                             PIC X(1).
               88  BUYNOW                           VALUE 'Y'.
               88  NOBUY                            VALUE 'N'.
           05  FILLER PIC X(12).
       01  WTO-MESSAGE PIC X(38) VALUE SPACES.
      * VSAM Record Layout
       01  PRODUCT.
           05  ITEMNUM     PIC X(10).
           05  FILLER    PIC X VALUE SPACES.
           05  DESCRIPTION   PIC X(34).
           05  FILLER    PIC X VALUE SPACES.
           05  TLABEL    PIC X(10).
           05  FILLER    PIC X VALUE SPACES.
           05  TAMOUNT.
               10  TAMT-SIGN                PIC X.
                   88 TAMT-SIGN-POSITIVE    VALUE '+'.
                   88 TAMT-SIGN-NEGATIVE    VALUE '-'.
               10  TAMT-INTEGER-PART        PIC X(8).
               10  TAMT-DEC-POINT           PIC X.
               10  TAMT-DECIMAL-PART        PIC X(8).
      *
       COPY MCMENU.
       COPY DFHAID.
       COPY DFHBMSCA.
       LINKAGE SECTION.
      *
       01  DFHCOMMAREA                       PIC X(14).
      *
       PROCEDURE DIVISION.
       MAIN.
           PERFORM HANDLE-AIDS.

           IF EIBAID = DFHPF1
                EXEC KICKS XCTL PROGRAM('MCHELP') END-EXEC
           ELSE
           IF EIBAID = DFHCLEAR OR DFHPF3
                EXEC CICS SEND CONTROL ERASE FREEKB END-EXEC
                EXEC CICS RETURN END-EXEC
           ELSE 
           IF EIBAID = DFHPA3
                MOVE 'Showing secret menu' TO WTO-MESSAGE
                PERFORM WTO
                EXEC CICS XCTL PROGRAM('SECRET') END-EXEC.

           IF EIBCALEN > ZERO THEN
               MOVE DFHCOMMAREA TO MC-COMMS
               MOVE 'Mels Cargo Main Menu' TO WTO-MESSAGE
               PERFORM WTO
           ELSE
           IF EIBCALEN EQUAL TO ZERO
               PERFORM SHOW-MENU.

           IF EIBAID EQUAL TO DFHENTER
                   PERFORM RECEIVE-OPTION
                   PERFORM PARSE-OPTION.

           EXEC CICS
               RETURN TRANSID('MCMM')
                      COMMAREA(MC-COMMS)
           END-EXEC.
       MC-EXIT.
           GOBACK.
      *
       RECEIVE-OPTION.
      * Get the option the user enters

           MOVE 'Getting Input from User.' TO WTO-MESSAGE.
           PERFORM WTO.
           EXEC CICS
               RECEIVE MAP('MENUMAP') MAPSET('MCMENU')
                       INTO(MENUMAPI)
           END-EXEC.

       PARSE-OPTION.
      *    Parse the user entry
           MOVE 'Parsing option' TO WTO-MESSAGE.
           PERFORM WTO.
           MOVE SELECTI TO WTO-MESSAGE.
           PERFORM WTO.

           IF SELECTI EQUAL TO '1'
               MOVE 'Opening Office Supplies Price List' TO WTO-MESSAGE
               PERFORM WTO
               EXEC CICS XCTL
                   PROGRAM('MCORDERS')
               END-EXEC
           ELSE
           IF SELECTI EQUAL TO '2'
               MOVE 'Opening Shipping Address' TO WTO-MESSAGE
               PERFORM WTO
               EXEC CICS XCTL
                   PROGRAM('MCSHIP')
               END-EXEC
           ELSE
           IF SELECTI EQUAL TO '3'
               MOVE 'Opening Order History' TO WTO-MESSAGE
               PERFORM WTO
               EXEC CICS XCTL
                   PROGRAM('MCOHIST')
               END-EXEC
           ELSE
           IF SELECTI EQUAL TO '99'
               MOVE 'Opening Such Send' TO WTO-MESSAGE
               PERFORM WTO
               EXEC CICS XCTL
                   PROGRAM('MCDELETE')
               END-EXEC.
           MOVE SPACES TO WTO-MESSAGE.
      *
      * Displays the Main Menu
      *
       SHOW-MENU.
           MOVE 'SHOWING MENU' TO WTO-MESSAGE.
           PERFORM WTO.
           EXEC CICS SEND MAP('MENUMAP') MAPSET('MCMENU') ERASE
           END-EXEC.
           MOVE 'SHOWING MENU - Done' TO WTO-MESSAGE.
           PERFORM WTO.
      *
      * WRITES A MESSAGE TO THE CONSOLE
      *
       WTO.
           EXEC CICS WRITE OPERATOR
               TEXT(WTO-MESSAGE)
           END-EXEC.
           MOVE SPACES TO WTO-MESSAGE.
      *
       HANDLE-AIDS.
      * This function handles all the AIDs
           IF EIBAID = DFHENTER
               MOVE '** AID: ENTER' TO WTO-MESSAGE
               PERFORM WTO
           ELSE
           IF EIBAID = DFHCLEAR
               MOVE '** AID: ENTER' TO WTO-MESSAGE
               PERFORM WTO
           ELSE
           IF EIBAID = DFHPA1
               MOVE '** AID: PA1' TO WTO-MESSAGE
               PERFORM WTO
